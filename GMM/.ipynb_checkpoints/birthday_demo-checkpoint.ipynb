   "source": [
    "function birthday(times::Int)\n",
    "    res = 0\n",
    "\tfor t = 1:times\n",
    "\t\tmonths = months = rand(1:12, 20)\n",
    "\t\tcount = [sum(months .== i) for i = 1:12]\n",
    "\t\tres += sum(count .== 2) == 4 && sum(count .== 3) == 4\n",
    "\tend\n",
    "\tres / times\n",
    "end"
   ]
  },
